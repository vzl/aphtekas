<h1>Listing category_products</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @category_products.each do |category_product| %>
  <tr>
    <td><%= category_product.name %></td>
    <td><%= category_product.url %></td>
    <td><%= link_to 'Show', category_product %></td>
    <td><%= link_to 'Edit', edit_category_product_path(category_product) %></td>
    <td><%= link_to 'Destroy', category_product, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Category product', new_category_product_path %>
